    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-editor-js": "2.1.0",
    "react-modal": "3.16.1",
    "react-toastify": "9.1.3",
    "sass": "1.69.5",
    "slate": "0.59.0",
    "slate-history": "0.59.0",
    "slate-react": "0.59.0",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "devDependencies": {
    "@editorjs/table": "2.3.0",
    "@types/jest": "29.5.8",
    "ajv": "8.12.0",
    "vitest": "1.0.0-beta.5"
